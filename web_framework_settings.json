{
  "WebServer": {
    "ip": "0.0.0.0",
    "port": 80,
    "timeout": 0
  },
  "LoadBalancer": {
    "heuristic": "Connections",
    "loadSource": "current",
    "serversHTTPS": false,
    "listOfServers": {
      "127.0.0.1": [
