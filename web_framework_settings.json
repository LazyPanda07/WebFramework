{
  "WebServer": {
    "ip": "0.0.0.0",
    "port": "80",
    "timeout": 0
  },
  "WebFramework": {
    "settingsPaths": [
      "web.json"
    ],
    "loadSources": [
      "current"
    ],
    "assetsPath": "assets",
    "templatesPath": "templates",
    "cachingSize": 0,
    "webServerType": "threadPool",
    "HTTPS": {
      "useHTTPS": false,
      "pathToCertificate": "certificates/cert.pem",
      "pathToKey": "certificates/key.pem"
    }
  },
  "Logging": {
    "usingLogging": true,
    "dateFormat": "DMY",
    "addNewLineAfterLog": true
  },
  "ThreadPoolServer": {
    "threadCount": 0
  }
}