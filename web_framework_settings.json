{
  "WebServer": {
    "ip": "0.0.0.0",
    "port": "80",
    "timeout": 0
  },

  "WebFramework": {
    "settingsPaths": [
      "web.json"
    ],
    "loadSources": [
      "current"
    ],
    "assetsPath": "assets",
    "templatesPath": "templates",
    "usingAssetsCache": true,
    "webServerType": "multithreaded"
  },

  "Logging": {
    "usingLogging": true,
    "dateFormat": "DMY",
    "addNewLineAfterLog": true
  }
}